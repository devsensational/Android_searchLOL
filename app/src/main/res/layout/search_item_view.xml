<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:orientation="vertical"
        android:layout_marginStart="10dp">

        <TextView
            android:id="@+id/win_textView"
            android:layout_width="26dp"
            android:layout_height="17dp"
            android:gravity="center"
            android:text="ìŠ¹" />

        <TextView
            android:id="@+id/gametime_textView"
            android:layout_width="24dp"
            android:layout_height="19dp"
            android:text="10:10"
            android:textSize="9sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="46dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:layout_weight="2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/championImageView"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/spell_imageView"
                        android:layout_width="15dp"
                        android:layout_height="13dp"
                        tools:srcCompat="@tools:sample/avatars" />
                    <ImageView
                        android:id="@+id/spell2_imageView"
                        android:layout_width="15dp"
                        android:layout_height="13dp"
                        tools:layout_editor_absoluteX="126dp"
                        tools:layout_editor_absoluteY="52dp"
                        tools:srcCompat="@tools:sample/avatars" />
                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/runes_imageView"
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    tools:layout_editor_absoluteX="141dp"
                    tools:layout_editor_absoluteY="52dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <ImageView
                    android:id="@+id/runes2_imageView"
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    tools:layout_editor_absoluteX="66dp"
                    tools:layout_editor_absoluteY="34dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <TextView
                    android:id="@+id/kda_textView"
                    android:layout_width="48dp"
                    android:layout_height="13dp"
                    android:text="10/10/10"
                    android:textSize="10sp"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="30dp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/item_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="51dp"
                tools:layout_editor_absoluteY="4dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/item2_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="51dp"
                tools:layout_editor_absoluteY="34dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/item3_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="51dp"
                tools:layout_editor_absoluteY="52dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/item4_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="66dp"
                tools:layout_editor_absoluteY="52dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/item5_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="81dp"
                tools:layout_editor_absoluteY="52dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/item6_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="96dp"
                tools:layout_editor_absoluteY="52dp"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/totem_imageView"
                android:layout_width="15dp"
                android:layout_height="13dp"
                tools:layout_editor_absoluteX="111dp"
                tools:layout_editor_absoluteY="52dp"
                tools:srcCompat="@tools:sample/avatars" />
        </LinearLayout>
    </LinearLayout>







</LinearLayout>